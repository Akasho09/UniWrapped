{
  "name": "@akasho9/drift2",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "db:generate" : "cd packages/db && npx prisma migrate dev --name init && npx prisma generate && cd ../.." ,
    "start-user-app" : "cd apps/user-app && npm run start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "next-auth": "^4.24.11"
  }
}
